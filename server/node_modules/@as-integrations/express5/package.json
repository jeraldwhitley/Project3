{
  "name": "@as-integrations/express5",
  "description": "An Apollo Server integration for use with Express v5",
  "version": "1.0.0",
  "author": "Apollo <packages@apollographql.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/apollo-server-integrations/apollo-server-integration-express5"
  },
  "homepage": "https://github.com/apollo-server-integrations/apollo-server-integration-express5",
  "bugs": {
    "url": "https://github.com/apollo-server-integrations/apollo-server-integration-express5/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "git clean -dfqX",
    "prepack": "npm run build",
    "prettier-check": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "changeset-publish": "npm run build && changeset publish",
    "changeset-check": "changeset status --verbose --since=origin/main",
    "changeset-version": "changeset version && npm i",
    "spell-check": "cspell lint '**' --no-progress || (echo 'Add any real words to cspell-dict.txt.'; exit 1)",
    "test": "jest",
    "test:ci": "jest --coverage --ci --maxWorkers=2 --reporters=default --reporters=jest-junit",
    "watch": "tsc --build --watch",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@apollo/server": "4.12.0",
    "@apollo/server-integration-testsuite": "4.12.0",
    "@apollo/utils.withrequired": "3.0.0",
    "@changesets/changelog-github": "0.5.1",
    "@changesets/cli": "2.29.3",
    "@eslint/js": "9.26.0",
    "@types/compression": "1.7.5",
    "@types/cors": "2.8.17",
    "@types/express": "5.0.1",
    "@types/express-serve-static-core": "5.0.6",
    "@types/jest": "29.5.14",
    "@types/node": "20.14.8",
    "@types/supertest": "6.0.3",
    "compression": "1.8.0",
    "cors": "2.8.5",
    "cspell": "9.0.0",
    "eslint": "9.26.0",
    "express": "5.1.0",
    "graphql": "16.11.0",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "prettier": "3.5.3",
    "supertest": "7.1.0",
    "ts-jest": "29.3.2",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.32.0"
  },
  "peerDependencies": {
    "@apollo/server": "^4.0.0",
    "express": "^5.0.0"
  }
}
